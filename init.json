{
#    "numberOfObject" : 4,
#
#    "projectName"    : "AccuControl",

    "PowerPlant":
    {
        "class":   "Worker.PowerPlant.PowerPlant"
    },

    "MqttBridge":
    {
        "class":     "MqttBridge.MqttBridge.MqttBridge",
        "interfaces":
        {
            "Mosquitto" : {
                "connection": "Interface.Ethernet.MqttBroker.MqttBroker",
                "server":     "192.168.168.23",
                "port":       233
            }
        }
    },

    "WatchDog":
    {
        "class":     "WatchDog.WatchDog.WatchDog",
        "interfaces":
        {
            "Uart" :
            {			# will be named WatchDog_Uart
                "connection": "Interface.Uart.Uart.Uart",
                "interface":  "/dev/ttyUSB0",
                "settings":   "19200,8,N,1"
            }
        },
        "timeout": "10"
    },

    "Logger": {
        "class":      "Logger.Logger.Logger"        # optionally: Logger.LoggerOverwrite.LoggerOverwrite
    }
}


